/* pages/index/insex.wxss */
page{
  background-color: #fff;
}
.pages{
	display: flex;
  	justify-content: center;
  	font-size: 16px;
}
.container{
	margin-top: 200rpx;
	width: 212px;
}
.logo{
	font-size: 12px;
	color: rgb(125,125,125);
	
}
.logo text{
	display: block;
	width: 84px;
	margin: 32rpx auto;
}
.logo image{
	display: block;
	margin: 32rpx auto;
  	width: 50px;
 	height: 50px;
}
.search{
	width: 212px;
	height: auto;
	margin-bottom: 16px;
}
.search input{
	border: 1px solid;
	border-radius: 5px;
}
.button{
	float: right;
}